{
  "table": "sample_merge_tree",
  "columns": [
    {
      "name": "dt",
      "type": {
        "name": "DateTime"
      }
    },
    {
      "name": "x",
      "type": {
        "name": "UInt64"
      }
    }
  ],
  "engine": {
    "name": "MergeTree"
  },
  "order_by": [
    {
      "parts": [
        "x"
      ]
    }
  ],
  "partition_by": {
    "name": "toYYYYMMDD",
    "arguments": [
      {
        "parts": [
          "dt"
        ]
      }
    ]
  },
  "settings": [
    {
      "name": "min_merge_bytes_to_use_direct_io",
      "value": {
        "type": "Integer",
        "value": 1
      }
    },
    {
      "name": "index_granularity",
      "value": {
        "type": "Integer",
        "value": 8192
      }
    }
  ],
  "settings_before_comment": true
}
